<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta http-equiv="X-UA-Compatible" content="ie=edge">
	<title>@ViewData["Title"] - ABCDMall</title>
	<link rel="icon" href="~/assets/img/file.jpg" type="image/png" asp-append-version="true">

	<link rel="stylesheet" href="~/vendors/bootstrap/bootstrap.min.css" asp-append-version="true">
	<link rel="stylesheet" href="~/vendors/fontawesome/css/all.min.css" asp-append-version="true">
	<link rel="stylesheet" href="~/vendors/themify-icons/themify-icons.css" asp-append-version="true">
	<link rel="stylesheet" href="~/vendors/nice-select/nice-select.css" asp-append-version="true">
	<link rel="stylesheet" href="~/vendors/owl-carousel/owl.theme.default.min.css" asp-append-version="true">
	<link rel="stylesheet" href="~/vendors/owl-carousel/owl.carousel.min.css" asp-append-version="true">

	<link rel="stylesheet" href="~/css/style.css" asp-append-version="true">
</head>
<body>

    <!--================ Start Header Menu Area =================-->
    <header class="header_area">
        <div class="main_menu">
            <nav class="navbar navbar-expand-lg navbar-light">
                <div class="container">
					<a class="navbar-brand logo_h" asp-controller="Home" asp-action="Index"><img src="~/assets/img/file.jpg" alt="" asp-append-version="true"><span><strong>ABCD Mall</strong></span></a>
                    <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarSupportedContent"
                            aria-controls="navbarSupportedContent" aria-expanded="false" aria-label="Toggle navigation">
                        <span class="icon-bar"></span>
                        <span class="icon-bar"></span>
                        <span class="icon-bar"></span>
                    </button>
                    <div class="collapse navbar-collapse offset" id="navbarSupportedContent">
                        <ul class="nav navbar-nav menu_nav ml-auto mr-auto">
                            <li class="nav-item">
								<a class="nav-link" asp-area="" asp-controller="Home" asp-action="Index">Home</a>
							</li>
							<li class="nav-item">
								<a class="nav-link" asp-area="" asp-controller="Home" asp-action="Shops">Shops</a>
							</li>
							<li class="nav-item">
								<a class="nav-link" asp-area="" asp-controller="Home" asp-action="Movies">Entertainment</a>
							</li>
							<li class="nav-item">
								<a class="nav-link" asp-area="" asp-controller="Home" asp-action="FoodCourts">Food Court</a>
							</li>
							<li class="nav-item">
								<a class="nav-link" asp-area="" asp-controller="Home" asp-action="Gallery">Gallery</a>
							</li>
							<li class="nav-item">
								<a class="nav-link" asp-area="" asp-controller="Home" asp-action="About">About</a>
							</li>
							<li class="nav-item">
								<a class="nav-link" asp-area="" asp-controller="Home" asp-action="Contact">Contact</a>
							</li>
							<li class="nav-item">
								<a class="nav-link" asp-area="" asp-controller="Home" asp-action="SubmitFeedback">Feedback</a>
							</li>
							<li class="nav-item">
								<a class="nav-link" asp-area="" asp-controller="Admin" asp-action="Login">Login</a>
							</li>
                        </ul>
                    </div>
                </div>
            </nav>
        </div>
    </header>
    <!--================ End Header Menu Area =================-->


        @RenderBody()

	
	<!--================ Start footer Area  =================-->
	<footer class="footer">
		<div class="footer-area">
			<div class="container">
				<div class="row section_gap">
					<div class="col-lg-3 col-md-6 col-sm-6">
						<div class="single-footer-widget tp_widgets">
							<h4 class="footer_title large_title">Our Mission</h4>
							<p>
								So seed seed green that winged cattle in. Gathering thing made fly you're no
								divided deep moved us lan Gathering thing us land years living.
							</p>
							<p>
								So seed seed green that winged cattle in. Gathering thing made fly you're no divided deep moved
							</p>
						</div>
					</div>
					<div class="offset-lg-1 col-lg-2 col-md-6 col-sm-6">
						<div class="single-footer-widget tp_widgets">
							<h4 class="footer_title">Quick Links</h4>
							<ul class="list">
								<li><a asp-controller="Home" asp-action="Index">Home</a></li>
								<li><a asp-controller="Home" asp-action="Shops">Shops</a></li>
								<li><a asp-controller="Home" asp-action="Movies">Movies</a></li>
								<li><a asp-controller="Home" asp-action="FoodCourts">Food Courts</a></li>
								<li><a asp-controller="Home" asp-action="Gallery">Gallery</a></li>
								<li><a asp-controller="Home" asp-action="About">About</a></li>
								<li><a asp-controller="Home" asp-action="Contact">Contact</a></li>
								<li><a asp-controller="Home" asp-action="Feedback">Feedback</a></li>
							</ul>
						</div>
					</div>
					<div class="col-lg-2 col-md-6 col-sm-6">
						<div class="single-footer-widget instafeed">
							<h4 class="footer_title">Gallery</h4>
							<ul class="list instafeed d-flex flex-wrap">
								<li><img src="~/img/gallery/r1.jpg" alt="" asp-append-version="true"></li>
								<li><img src="~/img/gallery/r2.jpg" alt="" asp-append-version="true"></li>
								<li><img src="~/img/gallery/r3.jpg" alt="" asp-append-version="true"></li>
								<li><img src="~/img/gallery/r5.jpg" alt="" asp-append-version="true"></li>
								<li><img src="~/img/gallery/r7.jpg" alt="" asp-append-version="true"></li>
								<li><img src="~/img/gallery/r8.jpg" alt="" asp-append-version="true"></li>
							</ul>
						</div>
					</div>
					<div class="offset-lg-1 col-lg-3 col-md-6 col-sm-6">
						<div class="single-footer-widget tp_widgets">
							<h4 class="footer_title">Contact Us</h4>
							<div class="ml-40">
								<p class="sm-head">
									<span class="fa fa-location-arrow"></span>
									Head Office
								</p>
								<p>123, Main Street, Your City</p>

								<p class="sm-head">
									<span class="fa fa-phone"></span>
									Phone Number
								</p>
								<p>
									+123 456 7890 <br>
									+123 456 7890
								</p>

								<p class="sm-head">
									<span class="fa fa-envelope"></span>
									Email
								</p>
								<p>
									free@infoexample.com <br>
									www.infoexample.com
								</p>
							</div>
						</div>
					</div>
				</div>
			</div>
		</div>

		<div class="footer-bottom">
			<div class="container">
				<div class="row d-flex">
					<p class="col-lg-12 footer-text text-center">
						Copyright &copy;<script>document.write(new Date().getFullYear());</script> All rights reserved By ABCD Mall
					</p>
				</div>
			</div>
		</div>
	</footer>
	<!--================ End footer Area  =================-->



	<script src="~/vendors/jquery/jquery-3.2.1.min.js" asp-append-version="true"></script>
	<script src="~/vendors/bootstrap/bootstrap.bundle.min.js" asp-append-version="true"></script>
	<script src="~/vendors/skrollr.min.js" asp-append-version="true"></script>
	<script src="~/vendors/owl-carousel/owl.carousel.min.js" asp-append-version="true"></script>
	<script src="~/vendors/nice-select/jquery.nice-select.min.js" asp-append-version="true"></script>
	<script src="~/vendors/jquery.ajaxchimp.min.js" asp-append-version="true"></script>
	<script src="~/vendors/mail-script.js" asp-append-version="true"></script>
	<script src="~/js/main.js" asp-append-version="true"></script>

	@* Render the Scripts section if it is defined in the view *@
	@RenderSection("Scripts", required: false)
</body>

</html>
