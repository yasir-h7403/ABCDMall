@model IEnumerable<ABCDMall.Models.Feedback>

<div class="container mt-5">
    <h2>Feedback List</h2>
    <table class="table">
        <thead>
            <tr>
                <th>User Name</th>
                <th>Comments</th>
                <th>Created At</th>
                <th>Updated At</th>
            </tr>
        </thead>
        <tbody>
            @foreach (var feedback in Model)
            {
                <tr>
                    <td>@feedback.UserName</td>
                    <td>@feedback.Comments</td>
                    <td>@feedback.CreatedAt.ToString("f")</td>
                    <td>@feedback.UpdatedAt.ToString("f")</td>
                </tr>
            }
        </tbody>
    </table>
</div>
